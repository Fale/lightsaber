[alias]
    l = "!source ~/.githelpers && pretty_git_log"
    amend = commit --amend
    aa = add --all
    head = !git l -1
    h = !git head
    hp = "!source ~/.githelpers && show_git_head"
    r = !git l
    ra = !git r --all
    ff = merge --ff-only
    pullff = pull --ff-only
    noff = merge --no-ff
    la = !git l --all
    div = divergence
    gn = goodness
    gnc = goodness --cached
    fa = fetch --all
    pa = push --all
    b = branch
    ds = diff --stat=160,120
    dh1 = diff HEAD~1
